.error_messages_box
  = render "shared/errors",obj: @member
%table.form_input_box
  %tr.form_input_box_content
    %th.form_input_box_label
      = f.label :名前
    %td.form_input_box_name
      = f.text_field :user_name, autocomplete: "first_name_kanji", placeholder: "例) 中 光夫", class: "form-input-box"
  %tr.form_input_box_content
    %th.form_input_box_label
      = f.label :メール
    %td.form_input_box_email
      = f.email_field :email, autofocus: true, placeholder: "例) PC・携帯どちらでも可"
  %tr.form_input_box_content
    %th.form_input_box_label
      = f.label :パスワード
    %td.form_input_box_password
      = f.password_field :password, autocomplete: "new-password", placeholder: "6文字以上"
  %tr.form_input_box_content
    %th.form_input_box_label
      = f.label :再度パスワード
    %td.form_input_box_password_confirmation
      = f.password_field :password_confirmation, autocomplete: "new-password", placeholder: "6文字以上"
  %tr.form_input_box_content
    %th.form_input_box_label
      = f.label :管理者権限
    %td.form_input_box_adiministrator
      = f.check_box :administrator
  %tr.form_input_box_content
    %th.form_input_box_label
      = f.label :所属チーム
    %td.form_input_box_room_id
      = f.collection_select :room_id, Room.all, :id, :name, {prompt: "--"}, {class: "select-wrap__holder"}

